<config>
    <modules>
        <Payone_Core>
            <version>3.7.0</version>
        </Payone_Core>
    </modules>

    <global>
        <blocks>
            <payone_core>
                <class>Payone_Core_Block</class>
            </payone_core>

            <checkout>
                <rewrite>
                    <onepage_payment_methods>Payone_Core_Block_Checkout_Onepage_Payment_Methods</onepage_payment_methods>
                </rewrite>
            </checkout>
        </blocks>

        <helpers>
            <payone_core>
                <class>Payone_Core_Helper</class>
            </payone_core>
        </helpers>

        <models>
            <payone_core>
                <class>Payone_Core_Model</class>
                <resourceModel>payone_core_resource</resourceModel>
            </payone_core>
            <payone_core_resource>
                <class>Payone_Core_Model_Domain_Resource</class>
                <entities>
                    <transaction>
                        <table>payone_transaction</table>
                    </transaction>
                    <protocol_transactionStatus>
                        <table>payone_protocol_transactionstatus</table>
                    </protocol_transactionStatus>
                    <protocol_api>
                        <table>payone_protocol_api</table>
                    </protocol_api>
                    <config_payment_method>
                        <table>payone_config_payment_method</table>
                    </config_payment_method>
                    <customer>
                        <table>payone_customer</table>
                    </customer>
                    <ratepay_config>
                        <table>payone_ratepay_config</table>
                    </ratepay_config>
                </entities>
            </payone_core_resource>

            <sales>
                <rewrite>
                    <quote_address>Payone_Core_Model_Sales_Quote_Address</quote_address>
                </rewrite>
            </sales>
        </models>

        <resources>
            <payone_core_setup>
                <setup>
                    <module>Payone_Core</module>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </payone_core_setup>
            <payone_core_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </payone_core_write>
            <payone_core_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </payone_core_read>
        </resources>
        <rewrite>
            <!-- Rewrite controller action from old Payone extension to Payone_Core
                 To allow old transactions to be finished. -->
            <payone_core_transaction_status>
                <from><![CDATA[#^/payone/processing/status#]]></from>
                <to>/payone_core/transactionStatus/</to>
            </payone_core_transaction_status>
        </rewrite>
        <payment>
            <groups>
                <payone>PAYONE</payone>
            </groups>
            <cc>
                <types>
                    <MCI>
                        <code>MCI</code>
                        <name>Maestro (International)</name>
                        <order>53</order>
                    </MCI>
                    <JCB>
                        <code>JCB</code>
                        <name>JCB</name>
                        <order>55</order>
                        <is_cvv_required>0</is_cvv_required>
                    </JCB>
                </types>
            </cc>
        </payment>
        <payone>
            <urls>
                <api>https://api.pay1.de/post-gateway/</api>
            </urls>
            <payment>
            </payment>
        </payone>

        <sales>
            <quote>
                <totals>
                    <payone_fee>
                        <class>payone_core/sales_quote_address_total_fee</class>
                        <after>shipping</after>
                    </payone_fee>
                </totals>
            </quote>
        </sales>

        <fieldsets>
            <sales_convert_quote_payment>
                <payone_config_payment_method_id>
                    <to_order_payment>*</to_order_payment>
                </payone_config_payment_method_id>
                <payone_safe_invoice_type>
                    <!-- Financing: Type -->
                    <to_order_payment>*</to_order_payment>
                </payone_safe_invoice_type>
                <payone_klarna_campaign_code>
                    <!-- Financing Klarna: Campaign Code -->
                    <to_order_payment>*</to_order_payment>
                </payone_klarna_campaign_code>
                <payone_onlinebanktransfer_type>
                    <!-- Online Bank Transfer: Type -->
                    <to_order_payment>*</to_order_payment>
                </payone_onlinebanktransfer_type>
                <payone_account_number>
                    <!-- Account number for: OnlineBankTransfer, DebitPayment -->
                    <to_order_payment>*</to_order_payment>
                </payone_account_number>
                <payone_account_owner>
                    <!-- Account owner for: DebitPayment -->
                    <to_order_payment>*</to_order_payment>
                </payone_account_owner>
                <payone_bank_country>
                    <!-- Online Bank Transfer: Bank Country -->
                    <to_order_payment>*</to_order_payment>
                </payone_bank_country>
                <payone_bank_code>
                    <!-- Bank Account Code for: OnlineBankTransfer, DebitPayment -->
                    <to_order_payment>*</to_order_payment>
                </payone_bank_code>
                <payone_sepa_iban>
                    <!-- Online Bank Transfer, DebitPayment: SEPA IBAN -->
                    <to_order_payment>*</to_order_payment>
                </payone_sepa_iban>
                <payone_sepa_bic>
                    <!-- Online Bank Transfer, DebitPayment: SEPA BIC -->
                    <to_order_payment>*</to_order_payment>
                </payone_sepa_bic>
                <payone_bank_group>
                    <!-- Online Bank Transfer: Bank Group -->
                    <to_order_payment>*</to_order_payment>
                </payone_bank_group>
                <payone_pseudocardpan>
                    <!-- Creditcard: Pseudo Card PAN-->
                    <to_order_payment>*</to_order_payment>
                </payone_pseudocardpan>
                <payone_customer_dob>
                    <!-- Safe Invoice Klarna: Customer Date Of Birth -->
                    <to_order_payment>*</to_order_payment>
                </payone_customer_dob>
                <payone_customer_gender>
                    <!-- Safe Invoice Klarna: Customer Gender -->
                    <to_order_payment>*</to_order_payment>
                </payone_customer_gender>
                <payone_customer_personalid>
                    <!-- Safe Invoice Klarna: Customer Personalid -->
                    <to_order_payment>*</to_order_payment>
                </payone_customer_personalid>
                <payone_billing_addressaddition>
                    <!-- Safe Invoice Klarna: Billing Address Addition -->
                    <to_order_payment>*</to_order_payment>
                </payone_billing_addressaddition>
                <payone_shipping_addressaddition>
                    <!-- Safe Invoice Klarna: Shipping Address Addition -->
                    <to_order_payment>*</to_order_payment>
                </payone_shipping_addressaddition>
                <payone_customer_telephone>
                    <!-- Safe Invoice Klarna: Customer Telephone -->
                    <to_order_payment>*</to_order_payment>
                </payone_customer_telephone>
                <payone_ratepay_shop_id>
                    <!-- RatePay: Shop (config) ID -->
                    <to_order_payment>*</to_order_payment>
                </payone_ratepay_shop_id>
                <payone_payolution_type>
                    <!-- Payolution type -->
                    <to_order_payment>*</to_order_payment>
                </payone_payolution_type>
                <payone_payolution_iban>
                    <!-- Payolution iban -->
                    <to_order_payment>*</to_order_payment>
                </payone_payolution_iban>
                <payone_payolution_bic>
                    <!-- Payolution bic -->
                    <to_order_payment>*</to_order_payment>
                </payone_payolution_bic>
                <payone_isb2b>
                    <!-- B2B mode ? -->
                    <to_order_payment>*</to_order_payment>
                </payone_isb2b>
                <payone_trade_registry_number>
                    <!-- Trade registry number -->
                    <to_order_payment>*</to_order_payment>
                </payone_trade_registry_number>
                <payone_cardexpiredate>
                    <!-- Creditcard cardexpiredate -->
                    <to_order_payment>*</to_order_payment>
                </payone_cardexpiredate>
                <payone_payolution_installment_duration>
                    <!-- Payolution installment duration -->
                    <to_order_payment>*</to_order_payment>
                </payone_payolution_installment_duration>
            </sales_convert_quote_payment>
            <sales_convert_quote_address>
                <payone_addresscheck_score>
                    <to_customer_address>*</to_customer_address>
                </payone_addresscheck_score>
                <payone_protect_score>
                    <to_customer_address>*</to_customer_address>
                </payone_protect_score>
                <payone_protect_date>
                    <to_customer_address>*</to_customer_address>
                </payone_protect_date>
            </sales_convert_quote_address>
        </fieldsets>

        <template>
            <email>
                <payone_misc_email_error_template translate="label" module="payone_core">
                    <label>PAYONE - Email Error</label>
                    <file>payone/core/error_template.phtml</file>
                    <type>html</type>
                </payone_misc_email_error_template>
                <payone_misc_email_avs_template translate="label" module="payone_core">
                    <label>PAYONE - Email AVS</label>
                    <file>payone/core/avs_template.phtml</file>
                    <type>html</type>
                </payone_misc_email_avs_template>
            </email>
        </template>

        <events>
            <sales_quote_address_validate_after>
                <observers>
                    <payone_core_observer>
                        <type>singleton</type>
                        <class>payone_core/observer_sales_quote_address</class>
                        <method>validateAfter</method>
                    </payone_core_observer>
                </observers>
            </sales_quote_address_validate_after>
            <sales_quote_payment_import_data_before>
                <observers>
                    <payone_core_observer>
                        <type>singleton</type>
                        <class>payone_core/observer_sales_quote_payment</class>
                        <method>importDataBefore</method>
                    </payone_core_observer>
                </observers>
            </sales_quote_payment_import_data_before>
            <checkout_onepage_payment_methods_get_methods>
                <observers>
                    <payone_core_observer>
                        <type>singleton</type>
                        <class>payone_core/observer_checkout_onepage_payment_methods</class>
                        <method>getMethods</method>
                    </payone_core_observer>
                </observers>
            </checkout_onepage_payment_methods_get_methods>
            <website_save_after>
                <observers>
                    <payone_core_observer>
                        <type>singleton</type>
                        <class>payone_core/observer_store_paymentConfig</class>
                        <method>createByWebsite</method>
                    </payone_core_observer>
                </observers>
            </website_save_after>

            <store_save_after>
                <observers>
                    <payone_core_observer>
                        <type>singleton</type>
                        <class>payone_core/observer_store_paymentConfig</class>
                        <method>createByStore</method>
                    </payone_core_observer>
                </observers>
            </store_save_after>
            <payone_core_checkout_onepage_verify_payment>
                <observers>
                    <payone_core_observer>
                        <type>singleton</type>
                        <class>payone_core/observer_checkout_onepage</class>
                        <method>verifyPayment</method>
                    </payone_core_observer>
                </observers>
            </payone_core_checkout_onepage_verify_payment>
            <controller_action_predispatch_payone_core_checkout_onepage_verifyPayment>
                <observers>
                    <payone_core_observer>
                        <type>singleton</type>
                        <class>payone_core/observer_checkout_onepage_debitPayment</class>
                        <method>performChecks</method>
                    </payone_core_observer>
                </observers>
            </controller_action_predispatch_payone_core_checkout_onepage_verifyPayment>
            <controller_action_predispatch_checkout_onepage_savePayment>
                <observers>
                    <payone_core_observer>
                        <type>singleton</type>
                        <class>payone_core/observer_checkout_onepage_debitPayment</class>
                        <method>performChecks</method>
                    </payone_core_observer>
                </observers>
            </controller_action_predispatch_checkout_onepage_savePayment>
            <sales_order_resource_init_virtual_grid_columns>
                <observers>
                    <payone_core_observer>
                        <class>payone_core/observer_sales_order</class>
                        <method>updateOrderGrid</method>
                    </payone_core_observer>
                </observers>
            </sales_order_resource_init_virtual_grid_columns>
            <sales_order_payment_place_end>
                <observers>
                    <payone_core_observer>
                        <class>payone_core/observer_sales_order</class>
                        <method>paymentPlaceEnd</method>
                    </payone_core_observer>
                </observers>
            </sales_order_payment_place_end>
            <sales_order_place_after>
                <observers>
                    <payone_core_observer>
                        <class>payone_core/observer_sales_order</class>
                        <method>incrementSampleCounter</method>
                    </payone_core_observer>
                </observers>
            </sales_order_place_after>
            <sales_order_payment_cancel>
                <observers>
                    <payone_core_observer>
                        <class>payone_core/observer_sales_order</class>
                        <method>cancelPayment</method>
                    </payone_core_observer>
                </observers>
            </sales_order_payment_cancel>
            <payone_core_transactionstatus_appointed>
                <observers>
                    <payone_core_observer_orderConfirmation>
                        <type>singleton</type>
                        <class>payone_core/observer_transactionStatus_orderConfirmation</class>
                        <method>onAppointed</method>
                    </payone_core_observer_orderConfirmation>
                    <payone_core_observer_invoiceCreate>
                        <type>singleton</type>
                        <class>payone_core/observer_transactionStatus_invoiceCreate</class>
                        <method>onAppointed</method>
                    </payone_core_observer_invoiceCreate>
                </observers>
            </payone_core_transactionstatus_appointed>
            <payone_core_transactionstatus_paid>
                <observers>
                    <payone_core_observer_invoiceCreate>
                        <type>singleton</type>
                        <class>payone_core/observer_transactionStatus_invoiceCreate</class>
                        <method>onPaid</method>
                    </payone_core_observer_invoiceCreate>
                </observers>
            </payone_core_transactionstatus_paid>
            <payone_core_transactionstatus_reminder>
                <observers>
                    <payone_core_observer_reminder>
                        <type>singleton</type>
                        <class>payone_core/observer_transactionStatus_reminder</class>
                        <method>onReminder</method>
                    </payone_core_observer_reminder>
                </observers>
            </payone_core_transactionstatus_reminder>
            <sales_order_payment_capture>
                <observers>
                    <payone_core_observer_invoice>
                        <type>singleton</type>
                        <class>payone_core/observer_sales_order_invoice</class>
                        <method>prepareInvoice</method>
                    </payone_core_observer_invoice>
                </observers>
            </sales_order_payment_capture>
            <payone_core_capture_approved>
                <observers>
                    <payone_core_observer_invoice>
                        <type>singleton</type>
                        <class>payone_core/observer_sales_order_invoice</class>
                        <method>prepareSequencenumber</method>
                    </payone_core_observer_invoice>
                </observers>
            </payone_core_capture_approved>
            <payone_core_api_protocol_save_after>
                <observers>
                    <payone_core_observer_protocol_api>
                        <type>singleton</type>
                        <class>payone_core/observer_protocol_api</class>
                        <method>prepareApi</method>
                    </payone_core_observer_protocol_api>
                </observers>
            </payone_core_api_protocol_save_after>
            <payone_core_service_payment>
                <observers>
                    <payone_core_observer_protocol_api>
                        <type>singleton</type>
                        <class>payone_core/observer_protocol_api</class>
                        <method>updateApiData</method>
                    </payone_core_observer_protocol_api>
                </observers>
            </payone_core_service_payment>
            <admin_system_config_changed_section_payone_protect>
                <observers>
                    <payone_core_observer_config_protect>
                        <type>singleton</type>
                        <class>payone_core/observer_config_protect</class>
                        <method>checkAllStatuses</method>
                    </payone_core_observer_config_protect>
                </observers>
            </admin_system_config_changed_section_payone_protect>

            <sales_order_save_commit_after>
                <observers>
                    <downloadable_observer>
                        <class>downloadable/observer</class>
                        <method>setLinkStatus</method>
                    </downloadable_observer>
                </observers>
            </sales_order_save_commit_after>
        </events>
    </global>

    <admin>
        <routers>
            <adminhtml>
				<args>
					<modules>
						<payone_core before="Mage_Adminhtml">Payone_Core_Adminhtml</payone_core>
					</modules>
				</args>
			</adminhtml>
        </routers>
    </admin>

    <adminhtml>
        <layout>
            <updates>
                <payone_core>
                    <file>payone/core.xml</file>
                </payone_core>
                <payone_configuration>
                    <file>payone/configuration.xml</file>
                </payone_configuration>
                <payone_transaction>
                    <file>payone/transaction.xml</file>
                </payone_transaction>
            </updates>
        </layout>

        <translate>
            <modules>
                <Payone_Core>
                    <files>
                        <default>Payone_Core.csv</default>
                    </files>
                </Payone_Core>
            </modules>
        </translate>
        
    </adminhtml>

    <frontend>
        <routers>
            <payone_core>
                <use>standard</use>
                <args>
                    <module>Payone_Core</module>
                    <frontName>payone_core</frontName>
                </args>
            </payone_core>
            <checkout>
                <args>
                    <modules>
                         <Payone_Core before="Mage_Checkout">Payone_Core_Checkout</Payone_Core>
                    </modules>
                </args>
            </checkout>
        </routers>

        <layout>
            <updates>
                <payone_core>
                    <file>payone/core.xml</file>
                </payone_core>
            </updates>
        </layout>

        <translate>
            <modules>
                <Payone_Core>
                    <files>
                        <default>Payone_Core.csv</default>
                    </files>
                </Payone_Core>
            </modules>
        </translate>
    </frontend>

    <default>
